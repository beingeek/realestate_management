 "font_size": 14,
 "html": "<style>\n    @media print {\n        .print-heading {\n            page-break-before: always;\n            text-align: center;\n            margin: 0 auto;\n            max-width: 800px;\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n        }\n        .print-heading img {\n            height: 80px;\n        }\n        .text-container {\n            text-align: center;\n            flex-grow: 1;\n            margin-left: 10px; /* Adjust the margin as needed */\n        }\n    }\n</style>\n\n<!-- Page 1 -->\n<div class=\"print-heading\">\n    <img src=\"/private/files/logo.png\" alt=\"TBD\">\n    <div class=\"text-container\">\n        <h3>{{doc.company}}</h3>\n        <h4>Plot Booking</h4>\n    </div>\n</div>\n\n<!-- Additional content goes here -->\n\n    <!-- Content Section -->\n    <br>\n    <div class=\"row section-break\">\n        <div class=\"col-xs-6 column-break\">\n            <!-- Customer Details -->\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>Customer Name</label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.customer}} \n                </div>\n            </div>\n\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>Father Name</label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.father_name}} \n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>CNIC </label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.cnic}} \n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>Contact No.</label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.contact_no}} \n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>Address.</label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.address}} \n                </div>\n            </div>\n        </div>\n        \n        <div class=\"col-xs-6 column-break\">\n            <!-- Booking Date and Plot Details -->\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>Booking No & Date</label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.name}} - {{frappe.utils.formatdate(doc.posting_date)}}\n                </div>\n            </div>\n\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>Scheme</label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.project}}\n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>Plot Number</label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.plot_no}}\n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>Plot Area</label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.area}} - {{doc.uom}}\n                </div>\n            </div>\n            \n            <div class=\"row\">\n                <div class=\"col-xs-5 text-left\">\n                    <label>Plot Price & Feature</label>\n                </div>\n                <div class=\"col-xs-6 value\">\n                    {{doc.get_formatted(\"land_price\", doc) }} - {{doc.plot_feature}}\n                </div>\n            </div>   \n        </div>\n    </div>\n        \n        <div class=\"row\" style=\"text-align: center; font-weight: bold;\">\n            <h4>Payment Schedule</h4>\n        </div>\n        \n        <!-- Payment Schedule Table -->\n        <table class=\"table table-bordered\">\n            <tbody>\n                <tr>\n                    <th style=\"text-align: center;\">Sr</th>\n                    <th style=\"text-align: center;\">Ins. Due Date</th>\n                    <th style=\"text-align: left;\">Description</th>\n                    <th class=\"text-right\"> Ins. Amount</th>\n                </tr>\n                {%- for row in doc.payment_schedule -%}\n                <tr>\n                    <td style=\"width: 8%; text-align:center ;\">{{ row.idx }}</td>\n                    <td style=\"width: 20%;text-align:center\">{{frappe.utils.formatdate(row.date)}}</td>\n                    <td style=\"width: 30%; text-align: left;\">{{ row.installment_name }}</td>\n                    <td style=\"width: 15%; text-align: right;\">{{ row.get_formatted(\"amount\", doc) }}</td>\n                </tr>\n                {%- endfor -%}\n            </tbody>\n        </table>\n        \n      <!-- Total Installment Amount -->\n        <div class=\"row section-break\">\n            <div class=\"col-xs-6 column-break\"></div>\n            <div class=\"col-xs-6 column-break\">\n                <div class=\"row\">\n                    <div class=\"col-xs-6 text-left\" style=\"font-weight: bold;\">\n                        <label>Total </label>\n                    </div>\n                    <div class=\"col-xs-6 text-right value\" style=\"font-weight: bold;\">\n                        {{ doc.get_formatted(\"total_sales_amount\", doc) }}\n                    </div>\n                </div>\n            </div>\n\n    <div class=\"row\" style=\"text-align: center;\">\n        <h4 style=\"text-decoration: underline;\">Payment Schedule Break-up</h4>\n    </div>\n\n    <table class=\"table table-bordered\">\n        <tbody>\n            <tr>\n                <th style=\"text-align: center;\"> Booking Amount</th>\n                <th style=\"text-align: center;\">Total Monthly Installment</th>\n                <th style=\"text-align: center;\">Total Quarterly Installment</th>\n                <th style=\"text-align: center;\">Total Half Yearly Installment</th>\n                <th style=\"text-align: center;\">Total Yearly Installment</th>\n                <th style=\"text-align: center;\">Possession Amount</th>\n            </tr>\n <tr>\n                <td style=\"width: 14%; text-align: center;\">{{ doc.get_formatted(\"total_booking_amount\", doc) }}</td>\n                <td style=\"width: 16%; text-align: center;\">{{ doc.get_formatted(\"total_monthly_installment\", doc) }}</td>\n                <td style=\"width: 14%; text-align: center;\">{{ doc.get_formatted(\"total_quarterly_installment\", doc) }}</td>\n                <td style=\"width: 16%; text-align: center;\">{{ doc.get_formatted(\"total_half_yearly_installment\", doc) }}</td>\n                <td style=\"width: 12%; text-align: center;\">{{ doc.get_formatted(\"total_yearly_installment\", doc) }}</td>\n                <td style=\"width: 16%; text-align: center;\">{{ doc.get_formatted(\"total_possession_amount\", doc) }}</td>\n            </tr> \n\n\n        </tbody>\n    </table>\n</div>\n\n    \n\n\n    \n \n\n\n",
